@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	Layout = "Master.cshtml";
	var exercise = Model.Content.Children.Where(x => x.IsVisible()).First();
	var selection = CurrentPage.Site().FirstChild("ExerciseRepository").Children("exercise").Where("Visible");
}


	<section>
		<div class="container">
			<div class="row">
				<div id="exerciseCon" class="col-sm-8">
					<div id="partialDiv">
					
					</div>
				</div>

	
				<div class="col-sm-4">
					
					<ul class="categoryLi">
					<h3>Øvelser</h3>
					<hr>
						@foreach(var item in selection){
						<li data-url="@(item.Url)" >@item.Name
						</li>
						}
					<hr>
					</ul>	
				</div>
			</div>
		</div>
	</section>
	